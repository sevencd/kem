<?xml version="1.0" encoding="UTF-8" ?>  
<!DOCTYPE mapper PUBLIC   
    "-//mybatis.org//DTD Mapper 3.0//EN"  
    "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="cn.ilanhai.mybatismapper.userZdslog">
	<insert id="insertZdsLog">
		INSERT
		INTO
		user_zdslog
		(USER_ID,param,result,exception,createtime)
		VALUES
		(#{userId},#{param},#{result},#{exception},now());
	</insert>
</mapper>